{
  "id":"/${project.artifactId}",
  "container":{
    "type":"DOCKER",
    "volumes":[
      {
        "hostPath":"/etc/localtime",
        "containerPath":"/etc/localtime",
        "mode":"RO"
      },
      {
        "hostPath":"/usr/share/zoneinfo/",
        "containerPath":"/usr/share/zoneinfo/",
        "mode":"RO"
      }
    ],
    "docker":{
      "image":"template",
      "forcePullImage":true,
      "network":"BRIDGE",
      "portMappings":[
        {
          "containerPort":9000
        }
      ],
      "parameters":[
        {
          "key":"net",
          "value":"fly-net"
        }
      ]
    }
  },
 "instances": 1,
	"cpus": 1,
	"mem": 3072,
  "healthChecks":[
    {
      "protocol":"TCP",
      "portIndex":0,
      "gracePeriodSeconds":300,
      "intervalSeconds":60,
      "timeoutSeconds":20,
      "maxConsecutiveFailures":3
    }
  ]
}